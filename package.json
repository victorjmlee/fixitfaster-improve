{
  "name": "fixitfaster",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx next dev",
    "dev:lan": "npx next dev -H 0.0.0.0",
    "build": "npx next build",
    "start": "node scripts/start-server.js",
    "lint": "npx next lint",
    "agent:up": "docker compose --env-file .env.local up -d",
    "agent:down": "docker compose down",
    "agent:logs": "docker compose --env-file .env.local logs -f agent",
    "pipeline:setup": "bash scripts/setup-log-pipeline.sh",
    "pipeline:setup:py": "python3 scripts/setup-log-pipeline.py",
    "agent:up:full": "npm run agent:up && npm run pipeline:setup",
    "leaderboard:reset": "node scripts/reset-leaderboard.cjs",
    "up": "npm run agent:up && npm run dev"
  },
  "dependencies": {
    "dd-trace": "^5.8.0",
    "dotenv": "^16.4.5",
    "next": "14.2.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.35",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  }
}
